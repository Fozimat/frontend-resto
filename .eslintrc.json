{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": "off",
    "quotes": "off",
    "no-underscore-dangle": "off",
    "no-console": "off",
    "comma-dangle": "off",
    "indent": "off",
    "no-restricted-globals": "off",
    "operator-linebreak": "off",
    "import/no-extraneous-dependencies": "off",
    "implicit-arrow-linebreak": "off",
    "no-undef": "off",
    "max-len": "off",
    "consistent-return": "off",
    "no-prototype-builtins": "off"
  }
}
